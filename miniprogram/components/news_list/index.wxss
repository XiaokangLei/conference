.home-list-left {
  position: relative;
  width: 124px;
  height: 70px;
}

.loding {
  margin-top: 20px;
  text-align: center;
  line-height: 30px;
  color: #909399
}

.home-list-left>view:nth-child(1) {
  width: 124px;
  height: 100%;
  border-radius: 6px;
  position: absolute;
  overflow: hidden;
}

.home-list-left>view:nth-child(2) {
  width: 8px;
  height: 26px;
  text-align: center;
  background: rgba(255, 255, 255, 0.8);
  padding: 3px;
  padding-left: 6px;
  font-size: 13px;
  font-family: PingFang;
  color: rgba(51, 51, 51, 1);
  position: absolute;
  bottom: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.home-jfg button {
  background: #fff;
  margin: 0px;
  display: inline;
  position: initial;
  padding: 0px;
  color: rgb(246 150 4);
  margin-top: -5px;
  line-height: initial;
  font-weight: initial;
  height: 63px;
}

.home-list-right {
  width: calc(100% - 134px);
}

.home-list-bottom>view:nth-child(1) {
  color: rgba(153, 153, 153, 1);
  text-align: center;
  margin: 6px;
  margin-left: 0px;
  margin-bottom: 0px;
  margin-top: 1px;
  padding-top: 2.5px;
  font-family: PingFang;
}

.item-hover {
  background: rgb(230, 230, 230) !important;
}

.home-list {
  position: relative;
  background-color: #fff;
  padding: 20rpx;
  padding-top: 24rpx;
  padding-bottom: 0px;
}

.home-list_df {
  display: flex;
  justify-content: space-between;
}

.home-jcly {
  margin: 10px;
  padding: 10px;
  background: #fff;
  border-radius: 6px;
}

.dis {
  display: flex;
}

.home-list-type {
  position: absolute;
  padding: 2px 10px;
  top: 0px;
  font-size: 12px;
  left: 0px;
  color: #fff;
  background: linear-gradient(to top left, #f7ce9e, #d69c63);
  border-bottom-right-radius: 12px;
}

.home-list-type2 {
  padding: 2px 10px;
  bottom: 0px;
  font-size: 12px;
  margin-right: 15rpx;
  color: #fff;
  background: linear-gradient(to top left, #f7ce9e, #d69c63);
}

.icon_wz {
  padding-left: 5px;
  padding-right: 15px;
  font-size: 24rpx;
  overflow: hidden;
  height: 20px;
}

.pd {
  padding: 10px;
}

.dis>view {
  display: flex;
  line-height: 38rpx;
}

.home-jfg>view:nth-child(1) {
  height: 111px;
  overflow: hidden;
}

.home-list-dlimg>view:nth-child(1) {
  font-size: 30rpx;
  color: #2e3135;
  font-weight: 600;
  padding-bottom: 4px;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.home-list-bottom {
  display: flex;
  padding-bottom: 8rpx;
  border-bottom: 1px solid #EBEEF5;
  justify-content: space-between;
}

.home-list-right_tille {
  font-size: 30rpx;
  color: #2e3135;
  font-weight: 600;
  overflow: hidden;
  white-space: nowrap;
  padding-bottom: 8rpx;
  text-overflow: ellipsis;
}

.home-list-right_mec {
  font-size: 13px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-family: PingFang;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 20px;
}

.home-list-dlimg_mc {
  margin-top: 3px;
  font-size: 13px;
  overflow: hidden;
  margin-bottom: 10rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-family: PingFang;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 20px
}

.home-list-right_mc {
  font-size: 13px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-family: PingFang;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
